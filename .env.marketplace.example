# Marketplace Integration Configuration
# Add these environment variables to your .env file

# Marketplace API Configuration
MARKETPLACE_API_URL=https://marketplace.ctrl-alt-play.com/api/v1
MARKETPLACE_API_KEY=your_marketplace_api_key_here
MARKETPLACE_JWT_SECRET=your_marketplace_jwt_secret_here

# Service Identification
SERVICE_ID=panel-system
SERVICE_NAME=Ctrl-Alt-Play Panel

# Integration Settings
MARKETPLACE_TIMEOUT=30000
MARKETPLACE_RETRY_ATTEMPTS=3
MARKETPLACE_RETRY_DELAY=1000

# Rate Limiting
MARKETPLACE_RATE_LIMIT_WINDOW=900000
MARKETPLACE_RATE_LIMIT_MAX=100

# Feature Flags
ENABLE_MARKETPLACE_INTEGRATION=true
ENABLE_USER_SYNC=true
ENABLE_PLUGIN_PUBLISHING=true
ENABLE_MARKETPLACE_ANALYTICS=true

# Development/Testing
MARKETPLACE_TEST_MODE=false
MARKETPLACE_DEBUG=false

{
     "name": "ctrl-alt-play",
     "version": "1.6.0",
     "description": "A comprehensive web-based game server management system - Ctrl-Alt-Play Panel Server",
     "main": "dist/index.js",
     "bin": {
          "plugin-cli": "./bin/plugin-cli.js"
     },
     "scripts": {
          "dev": "nodemon src/index.ts",
          "build": "tsc",
          "start": "node dist/index.js",
          "test": "jest",
          "test:watch": "jest --watch",
          "test:coverage": "jest --coverage",
          "test:ci": "jest --ci --coverage --watchAll=false",
          "test:integration": "jest --testPathPattern=integration",
          "test:unit": "jest --testPathPattern='tests/unit'",
          "test:frontend": "cd frontend && npm test && cd ..",
          "lint": "eslint src/**/*.ts",
          "lint:fix": "eslint src/**/*.ts --fix",
          "type-check": "tsc --noEmit",
          "docker:build": "docker build -t game-panel .",
          "docker:run": "docker run -p 3000:3000 game-panel",
          "migrate": "prisma migrate dev",
          "db:generate": "prisma generate",
          "db:push": "prisma db push",
          "db:seed": "prisma db seed",
          "db:reset": "prisma migrate reset",
          "test:keploy": "jest --coverage --coverageReporters=text --coverageReporters=cobertura --collectCoverageFrom='./**/*.{js,jsx,ts,tsx}'",
          "plugin:create": "node scripts/plugin-tools/cli.js create",
          "plugin:install": "node scripts/plugin-tools/cli.js install",
          "plugin:list": "node scripts/plugin-tools/cli.js list",
          "plugin:enable": "node scripts/plugin-tools/cli.js enable",
          "plugin:disable": "node scripts/plugin-tools/cli.js disable",
          "plugin:validate": "node scripts/plugin-tools/cli.js validate",
          "plugin:test": "node test-plugin-system.js"
     },
     "keywords": [
          "game-server",
          "management",
          "panel",
          "web-interface",
          "nodejs",
          "typescript",
          "ctrl-alt-play",
          "docker",
          "agent-system"
     ],
     "author": "scarecr0w12 <https://github.com/scarecr0w12>",
     "license": "MIT",
     "dependencies": {
          "@prisma/client": "^6.12.0",
          "@types/inquirer": "^9.0.8",
          "@types/js-yaml": "^4.0.9",
          "axios": "^1.11.0",
          "bcrypt": "^6.0.0",
          "bcryptjs": "^3.0.2",
          "chalk": "^5.4.1",
          "chart.js": "^4.5.0",
          "chartjs-node-canvas": "^5.0.0",
          "chokidar": "^4.0.3",
          "commander": "^14.0.0",
          "compression": "^1.7.4",
          "cors": "^2.8.5",
          "dotenv": "^17.2.1",
          "express": "^5.1.0",
          "express-rate-limit": "^8.0.1",
          "express-validator": "^7.0.1",
          "helmet": "^8.1.0",
          "inquirer": "^12.8.2",
          "js-yaml": "^4.1.0",
          "jsonwebtoken": "^9.0.2",
          "multer": "^2.0.2",
          "node-cron": "^4.2.1",
          "prisma": "^6.12.0",
          "redis": "^5.6.1",
          "socket.io": "^4.7.4",
          "winston": "^3.11.0",
          "ws": "^8.18.3",
          "yaml": "^2.8.0"
     },
     "devDependencies": {
          "@types/bcrypt": "^6.0.0",
          "@types/bcryptjs": "^3.0.0",
          "@types/compression": "^1.7.5",
          "@types/cors": "^2.8.17",
          "@types/express": "^5.0.3",
          "@types/jest": "^30.0.0",
          "@types/jsonwebtoken": "^9.0.10",
          "@types/multer": "^2.0.0",
          "@types/node": "^24.1.0",
          "@types/node-cron": "^3.0.11",
          "@types/react": "^19.1.8",
          "@types/react-dom": "^19.1.6",
          "@types/supertest": "^6.0.3",
          "@types/ws": "^8.18.1",
          "@typescript-eslint/eslint-plugin": "^8.38.0",
          "@typescript-eslint/parser": "^8.38.0",
          "eslint": "^9.32.0",
          "jest": "^30.0.5",
          "nodemon": "^3.0.2",
          "supertest": "^7.1.4",
          "ts-jest": "^29.1.1",
          "ts-node": "^10.9.2",
          "typescript": "^5.3.3"
     },
     "prisma": {
          "seed": "node dist/prisma/seed.js"
     },
     "engines": {
          "node": ">=18.0.0"
     },
     "repository": {
          "type": "git",
          "url": "https://github.com/scarecr0w12/ctrl-alt-play-panel.git"
     },
     "bugs": {
          "url": "https://github.com/scarecr0w12/ctrl-alt-play-panel/issues"
     },
     "homepage": "https://github.com/scarecr0w12/ctrl-alt-play-panel#readme"
}
